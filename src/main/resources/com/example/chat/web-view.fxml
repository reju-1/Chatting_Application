<AnchorPane prefHeight="750.0" prefWidth="950.0" style="-fx-background-color: #435055;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.chat.WebView">
