<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox onMouseClicked="#selectContract" prefHeight="75.0" prefWidth="346.0" style="-fx-background-color: #282e33;" stylesheets="@stylesheet/style.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.chat.Contact">
   <children>
      <ImageView fx:id="image" fitHeight="50.0" fitWidth="45.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/prifile.png" />
         </image>
         <HBox.margin>
            <Insets left="10.0" top="10.0" />
         </HBox.margin>
      </ImageView>
      <Label fx:id="name" prefHeight="26.0" prefWidth="186.0" text="Rejuyan Ahmed" textFill="#ffffffde">
         <font>
            <Font size="15.0" />
         </font>
         <HBox.margin>
            <Insets left="20.0" top="20.0" />
         </HBox.margin>
         <padding>
            <Insets left="5.0" top="5.0" />
         </padding>
      </Label>
   </children>
   <padding>
      <Insets top="5.0" />
   </padding>
   <styleClass>
      <String fx:value="margin-top-bottom" />
      <String fx:value="contact" />
   </styleClass>
</HBox>
